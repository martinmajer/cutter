{include ./default-content.jtpl}
<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <title>{$title}</title>
    </head>
    <body>
        {cutout "body"}
            {virtual content}
            
            {for var i = 0; i < $array.length; i++}
                {cutout "row-"+i}
                    {$array[i]}
                {/}
            {/}
        {/}
        
        {section footer}
            &copy; 2013 default footer.
        {/}
        {virtual footer}
    </body>
</html>

